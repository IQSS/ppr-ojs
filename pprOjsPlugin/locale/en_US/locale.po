msgid "plugins.generic.pprPlugin.displayName"
msgstr "IQSS Peer Pre-Review Program Plugin"

msgid "plugins.generic.pprPlugin.description"
msgstr "Customizations to the OJS registration and submission workflow for use in the Peer Pre-Review Program at Harvardâ€™s IQSS"

msgid "plugins.generic.pprPlugin.settings.description"
msgstr "Settings and Feature Flags for the IQSS Peer Pre-Review Program Plugin."

msgid "plugins.generic.pprPlugin.settings.section.featureFlags"
msgstr "Feature Flags"

msgid "plugins.generic.pprPlugin.settings.section.workflow"
msgstr "Submission / Review Workflow"

msgid "plugins.generic.pprPlugin.settings.section.users"
msgstr "Users / Contributors"

msgid "plugins.generic.pprPlugin.settings.section.reviews"
msgstr "Reviews"

msgid "plugins.generic.pprPlugin.settings.section.submissions"
msgstr "Submissions"

msgid "plugins.generic.pprPlugin.settings.section.tasks"
msgstr "Scheduled Tasks"

msgid "plugins.generic.pprPlugin.settings.workflowMessage.label"
msgstr "Show the information message in the hidden \"Copyediting\" and \"Production\" tabs"

msgid "plugins.generic.pprPlugin.settings.contributors.label"
msgstr "Show the contributors panel in the workflow page"

msgid "plugins.generic.pprPlugin.settings.reviewers.label"
msgstr "Show the suggested reviewers panel in the workflow page"

msgid "plugins.generic.pprPlugin.settings.reviewMethod.label"
msgstr "Hide the review method field and default to \"Anonymous Reviewer/Anonymous Author\""

msgid "plugins.generic.pprPlugin.settings.reviewRecommendation.label"
msgstr "Hide the review recommendation field and default \"Revisions Required\""

msgid "plugins.generic.pprPlugin.settings.preferredPublicName.label"
msgstr "Hide \"Preferred Public Name\" field from the user and contributor form"

msgid "plugins.generic.pprPlugin.settings.userCustomFields.label"
msgstr "Add institutional position, department, and required fields to the registration, user, author, and reviewer forms"

msgid "plugins.generic.pprPlugin.settings.categoryOptions.label"
msgstr "Institutional position dropdown options. Comma separated list of values"

msgid "plugins.generic.pprPlugin.settings.institutionOptions.label"
msgstr "Institution dropdown options for registration form. Comma separated list of values"

msgid "plugins.generic.pprPlugin.settings.submissionCustomFields.label"
msgstr "Add comments for the reviewer to author submission form and show the data in the review"

msgid "plugins.generic.pprPlugin.settings.submissionCloseEnabled.label"
msgstr "Add close submission functionality to editorial actions component"

msgid "plugins.generic.pprPlugin.settings.submissionConfirmationChecklistEnabled.label"
msgstr "Add the submission checklist to the confirmation page"

msgid "plugins.generic.pprPlugin.settings.submissionUploadFileValidationEnabled.label"
msgstr "Add required upload submission file validation"

msgid "plugins.generic.pprPlugin.settings.submissionRequestRevisionsFileValidationEnabled.label"
msgstr "Add the revisions file validation to the Request Revisions form"

msgid "plugins.generic.pprPlugin.settings.reviewReminderEditorEnabled.label"
msgstr "Add review response/submit due date notification for associate editors"

msgid "plugins.generic.pprPlugin.settings.reviewReminderEditorReset.label"
msgstr "Trigger the execution of the review notifications for associate editors scheduled task"

msgid "plugins.generic.pprPlugin.settings.reviewReminderEditorDaysFromDueDate.label"
msgstr "Comma separated list. Number of days before (negative) or after (positive) the due date to send notifications to the editor"

msgid "plugins.generic.pprPlugin.settings.reviewReminderEditorDaysFromDueDate.invalid"
msgstr "The number of days allowed are values from -20 to 20"

msgid "user.category"
msgstr "Institutional Position"

msgid "user.category.description"
msgstr "Institutional Position: (e.g. Faculty, Student, etc.)"

msgid "user.department"
msgstr "Academic Department"

msgid "user.department.description"
msgstr "Academic Department:  (e.g. Government, Economics, etc.)"

msgid "submission.comments.reviewer"
msgstr "Comments for the Reviewer"

msgid "submission.comments.reviewer.description"
msgstr "If you'd like to direct the reviewer's attention to any particular area of the paper, please describe it here."

msgid "submission.close.button.title"
msgstr "Close Submission"

msgid "submission.close.form.title"
msgstr "Close Submission"

msgid "submission.open.button.title"
msgstr "Open Submission"

msgid "submission.open.form.title"
msgstr "Open Submission"

msgid "submission.close.form.description"
msgstr "This will update the status of the submission to closed and move it into the \"Archives\" section"

msgid "submission.open.form.description"
msgstr "This will update the status of the submission to open and move it into the \"My Queue\" section"

msgid "submission.ppr.confirmation.intro"
msgstr "Your document has been uploaded and is ready to be submitted. Please confirm that you have included the following items in your submission."

msgid "submission.ppr.confirmation.message"
msgstr "You may go back to review and adjust any of the information you have entered before continuing.<br/> When you are ready, click \"Finish Submission\"."

msgid "revisions.ppr.review.files.validation.message"
msgstr "No reviewer document has been selected to send to the author. Would you like to proceed? \n\nSelect \"OK\" to send the review without a reviewer document attached or \"Cancel\" to add a document on the \"Select review files to share with the author(s)\" form."

msgid "submission.ppr.files.validation.header"
msgstr "Missing Files"

msgid "submission.ppr.files.validation.description"
msgstr "Please upload your blinded research document before selecting \"Save and continue\"."

msgid "submission.ppr.files.validation.button"
msgstr "Close"
